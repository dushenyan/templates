{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "globalStyle": {
    "backgroundColor": "@bgColor",
    "backgroundColorBottom": "@bgColorBottom",
    "backgroundColorTop": "@bgColorTop",
    "backgroundTextStyle": "@bgTxtStyle",
    "navigationBarBackgroundColor": "#000000",
    "navigationBarTextStyle": "@navTxtStyle",
    "navigationBarTitleText": "Uni Creator",
    "navigationStyle": "custom"
  },
  "pages": [
    // GENERATED BY UNI-PAGES, PLATFORM: MP-WEIXIN || H5
    {
      "path": "pages/home",
      "type": "page",
      "style": {
        "navigationBarTitleText": "主页"
      },
      "name": "home",
      "layout": "tabbar"
    },
    {
      "path": "pages/playground",
      "type": "page",
      "style": {
        "navigationBarTitleText": "演练场"
      },
      "name": "playground",
      "layout": "tabbar"
    },
    {
      "path": "pages/setting",
      "type": "page",
      "style": {
        "navigationBarTitleText": "设置"
      },
      "name": "setting",
      "layout": "tabbar"
    },
    {
      "path": "pages/user",
      "type": "page",
      "style": {
        "navigationBarTitleText": "用户"
      },
      "name": "user",
      "layout": "tabbar"
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        // GENERATED BY UNI-PAGES, PLATFORM: MP-WEIXIN || H5
        {
          "path": "example/comp",
          "type": "page",
          "style": {
            "navigationBarTitleText": "用户"
          }
        },
        {
          "path": "example/hi",
          "type": "page",
          "style": {
            "navigationBarTitleText": "hi"
          }
        },
        {
          "path": "example/index",
          "type": "page"
        },
        {
          "path": "example/request",
          "type": "page",
          "style": {
            "navigationBarTitleText": "hi"
          }
        },
        {
          "path": "personal/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "用户"
          },
          "name": "pageSubPersonal"
        },
        {
          "path": "preference/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "用户"
          },
          "name": "pageSubPreference"
        },
        // #ifdef MP-WEIXIN
        {
          "path": "example/paing",
          "type": "page"
        },
        // #endif
        // #ifdef H5
        {
          "path": "example/paing",
          "type": "page",
          "style": {
            "navigationBarTitleText": "演练场"
          },
          "name": "playground"
        }
        // #endif
      ]
    }
  ],
  "tabBar": {
    "custom": true,
    "height": "0",
    "color": "#bfbfbf",
    "selectedColor": "#0165FF",
    "list": [
      // GENERATED BY UNI-PAGES, PLATFORM: MP-WEIXIN || H5
      {
        "pagePath": "pages/home"
      },
      {
        "pagePath": "pages/playground"
      },
      {
        "pagePath": "pages/setting"
      },
      {
        "pagePath": "pages/user"
      }
    ]
  }
}