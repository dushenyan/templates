{
  "name": "vite-plugin-template",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.17.1",
  "__description__": "开箱即用的 Vite 插件模板",
  "description": "description",
  "author": "dushenyan<dushenyan88@gmail.com>",
  "homepage": "https://pkg-name.netlify.app/",
  "repository": "https://github.com/dushenyan/pkg-name",
  "keywords": [
    "vite",
    "plugin",
    "template",
    "development",
    "typescript",
    "testing"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "client.d.ts",
    "dist"
  ],
  "scripts": {
    "dev": "tsdown",
    "build": "tsdown --minify",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ui": "npx @eslint/config-inspector",
    "type-check": "tsc --noEmit",
    "release": "npm run build && bumpp --commit --push --tag",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "play:preview": "pnpm -C playground run preview"
  },
  "peerDependencies": {
    "vite": "^3.0.0 || ^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@types/node": "^22.19.0",
    "@vitest/coverage-v8": "^4.0.6",
    "bumpp": "^10.3.1",
    "eslint": "^9.36.0",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vite": "^6.4.x",
    "vitest": "^4.0.6"
  }
}