{
  "publisher": "your-username",
  "name": "pkg-name",
  "displayName": "pkg-name",
  "version": "0.0.1",
  "packageManager": "pnpm@10.25.0",
  "__description__": "快速创建VS Code扩展模板项目",
  "description": "description",
  "author": "dushenyan<dushenyan88@gmail.com>",
  "homepage": "https://pkg-name.netlify.app/",
  "repository": "https://github.com/dushenyan/pkg-name",
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.106.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "PkgName.showHelloWorld",
        "title": "Hello World (Vue): Show"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "pkg-name",
      "properties": {}
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "PkgName",
          "title": "pkg-name",
          "icon": "res/icon.svg"
        }
      ]
    },
    "views": {
      "PkgName": [
        {
          "id": "PkgName.basic",
          "name": "pkg name configurator",
          "icon": "res/icon.svg",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "npm-run-all --parallel dev:webview dev:plugin",
    "dev:webview": "pnpm --filter webview-ui build --watch",
    "dev:plugin": "pnpm run build --watch --sourcemap",
    "update": "vscode-ext-gen --output src/generated-meta.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "bumpp": "bumpp",
    "vscode:package": "vsce package --no-dependencies",
    "vscode:publish": "pnpm run build && vsce publish --no-dependencies",
    "ovsx:publish": "pnpm run build && ovsx publish --no-dependencies",
    "clean": "rimraf dist webview-ui/dist"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@antfu/ni": "^23.3.1",
    "@types/node": "^22.19.2",
    "@types/vscode": "^1.106.1",
    "@vscode/vsce": "^3.7.1",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "esno": "^4.8.0",
    "npm-run-all": "^4.1.5",
    "ovsx": "^0.10.7",
    "pnpm": "^10.25.0",
    "reactive-vscode": "^0.2.22",
    "rimraf": "^6.1.2",
    "tsdown": "^0.9.9",
    "typescript": "^5.9.3",
    "vite": "^7.1.14",
    "vitest": "^3.2.4",
    "vscode-ext-gen": "^1.4.0",
    "vsxpub": "^0.1.2"
  }
}